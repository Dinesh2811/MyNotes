        <item name="colorPrimaryInverse">@color/Primary40</item>
        <item name="colorSecondary">@color/Secondary80</item>
        <item name="colorOnSecondary">@color/Secondary20</item>
        <item name="colorSecondaryContainer">@color/Secondary30</item>
        <item name="colorOnSecondaryContainer">@color/Secondary90</item>
        <item name="colorTertiary">@color/Tertiary80</item>
        <item name="colorOnTertiary">@color/Tertiary20</item>
        <item name="colorTertiaryContainer">@color/Tertiary30</item>
        <item name="colorOnTertiaryContainer">@color/Tertiary90</item>
        <item name="colorError">@color/Error80</item>
