        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

        <item name="backgroundColor">@color/Neutral10</item>

        <!--        Toolbar & Multi Select Rv         -->
        <item name="actionModeBackground">?attr/colorOnPrimary</item>
        <item name="homeAsUpIndicator">@drawable/ic_baseline_arrow_back_24</item>
        <item name="actionModeCloseDrawable">@drawable/ic_baseline_close_24</item>
        <item name="windowActionBarOverlay">true</item>
        <item name="windowActionModeOverlay">true</item>

